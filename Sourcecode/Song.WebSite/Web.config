  <!--站点的配置信息-->
  <Platform name="study" version="2.0">
    <!--应用程序的参数-->
    <App key="128087" version="2.0">
      <!--雪花ID的机器码，即每台服务器的编码-->
      <item key="Snowflake_WorkerID" value="1" />
      <!--日志等级，0.不输出日志；1.只输出错误信息; 2.输出错误和正常信息; 3.输出错误信息、正常信息和调试信息-->
      <item key="LOG_LEVEL" value="3" />
      <!--记录接口执行时间，单位秒，当执行时间大于等于value时才记录-->
      <item key="LOG_ApiElapsed" value="1" />
      <!--请求接口的js所在的页面与地址栏的地址，必须域名一致，为true则验证，false不验证；默认为true-->
      <item key="RESTfulAPI_HostCheck" value="true" />
      <!--应用程序状态，如果Debug，帮助信息可编辑-->
      <item key="AppState" value="Debug" />     
        <!--接口参数的过虑字符，会将英文字转换为全角字符-->
      <item key="FilterWords" value="'|..|>|and|exec|insert|select|delete|update|count|*|%|chr|mid|master|truncate|char|declare|script|frame|;|or|-|+|,|)|etc|style|expression" />
      <!--题型-->
      <item key="QuesType" value="单选, 多选, 判断, 简答, 填空" />
      <item key="ActivType" value="CPU串号,硬盘串号,IP+端口,域名+端口,主域名+端口" />
      <!--Excel导入时，记录列名与字段名对应关系的文件的存放路径-->
      <item key="ExcelInputConfig" value="~/Utilities/DataTemplate/Config" />        
      <item key="ExamFileUp" value="doc,docx,xls,xlsx,ppt,pptx,pdf,zip,rar,jpg,jpeg,gif,png,bmp" intro="考试时简答题上传附件的限定文件类型" />
    </App>
    <!--RESTfulAPI接口请求的配置项
    hostcheck:-请求接口的js所在的页面与地址栏的地址，必须域名一致，为true则验证，false不验证；默认为true
    -->
    <RESTfulAPI hostcheck="true">
      <!--白名单 value为true则启用，在hostcheck为true时，会通过白名单的地址-->
      <item key="white" value="true">
        <item  key="" value=""/>
      </item>
      <!--黑名单 value为true则启用，在hostcheck为true时，会禁止黑名单的地址来源的请求-->
      <item key="black" value="true">
        <item  key="" value=""/>
      </item>
    </RESTfulAPI>
    <Entity>
      <item key="Accounts" value="账户" visible="true" />
      <item key="Teacher" value="教师" />
