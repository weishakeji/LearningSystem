      <!--单点登录-->
      <item key="Sso" value="Sso" default="Default" />
      <item key="Tv" value="Tv" default="Default" />
      <item key="Pad" value="Pad" default="Default" />
    </Template>
    <!--资料上传的路径，path为根目录，子节点为各模块上传目录（相对于path属性的值）-->
